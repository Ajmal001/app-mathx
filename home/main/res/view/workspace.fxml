<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="1080.0" prefWidth="1920.0"
            style="-fx-background-color: #FFFFFF;" stylesheets="/main/res/css/workspace.css"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="main.src.controllers.WorkspaceController">
   <top>
      <HBox prefHeight="40.0" prefWidth="1920.0" style="-fx-background-color: #E3E3E3;" BorderPane.alignment="CENTER">
         <Pane prefHeight="45.0" prefWidth="135.0">
            <Label fx:id="homeButton" layoutX="38.0" layoutY="7.0" text="Home">
               <font>
                  <Font name="FreeSans Bold" size="14.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
         </Pane>
         <Pane prefHeight="45.0" prefWidth="135.0">
            <Label layoutX="38.0" layoutY="7.0" text="Reset">
               <font>
                  <Font name="FreeSans Bold" size="14.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
         </Pane>
      </HBox>
   </top>
   <left>
      <Pane prefHeight="1040.0" prefWidth="250.0" BorderPane.alignment="CENTER">
         <ScrollPane layoutY="-1.0" prefHeight="520.0" prefWidth="250.0">
            <VBox fx:id="sidePane" maxHeight="5000.0" minHeight="1000.0" prefHeight="1040.0" prefWidth="250.0"/>
         </ScrollPane>
      </Pane>
   </left>
   <center>
      <Pane fx:id="sandBox" style="-fx-background-color: #FFFFFF" BorderPane.alignment="CENTER"/>
   </center>
    <bottom>
       <Pane prefHeight="520.0" prefWidth="1024.0" style="-fx-background-color: #E3E3E3;"
             BorderPane.alignment="CENTER"/>
    </bottom>
</BorderPane>
